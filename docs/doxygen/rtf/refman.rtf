00055 {\cf18 void} sleep_ticks(uint32_t delay)\par
00056 \{\par
00057     uint64_t current_ticks = timer_get_ticks();\par
00058     {\cf19 while} (1)\par
00059     \{\par
00060         {\cf19 if} (current_ticks + delay < timer_get_ticks())\{\par
00061             {\cf19 break};\par
00062         \}\par
00063     \}\par
00064 \}\par
