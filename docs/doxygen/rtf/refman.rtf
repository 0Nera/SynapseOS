\'D1\'EC. \'E4\'EE\'EA\'F3\'EC\'E5\'ED\'F2\'E0\'F6\'E8\'FE.{
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 00001 \par
00014 {\cf21 #include <arch.h>}\par
00015 {\cf21 #include <kernel.h>}\par
00016 {\cf21 #include <libk.h>}\par
00017 {\cf21 #include <mod.h>}\par
00018 \par
00019 {\cf17 static} int (*entry_point)(module_syscalls_t *syscalls);\par
00020 \par
00030 {\cf18 int} elf_module_load(module_elf_programm_t *info, module_syscalls_t *syscalls) \{\par
00031     uint8_t *data;\par
00032     {\cf20 /*}\par
00033 {\cf20     char **final_argv = oxygen_alloc(sizeof(char**) * argc);    }\par
00034 {\cf20     }\par
00035 {\cf20     final_argv[0] = (char*)info->name;}\par
00036 {\cf20 }\par
00037 {\cf20     for(size_t i = 1; i < argc; i++) \{}\par
00038 {\cf20         final_argv[i] = argv[i-1];}\par
00039 {\cf20     \}}\par
00040 {\cf20     oxygen_free(final_argv);}\par
00041 {\cf20     */}\par
00042     \par
00043     elf_header_t *header = info->header;\par
00044     kprintf({\cf22 "[%s] at [0x%x] [%s]\\n"}, info->name, header->entry, header->magic);\par
00045     debug_log({\cf22 "[%s] at [0x%x] [%s]"}, info->name, header->entry, header->magic);\par
00046 \par
00047     {\cf19 if}(header->magic[0] != 0x7f OR\par
00048        header->magic[1] != {\cf23 'E'}  OR\par
00049        header->magic[2] != {\cf23 'L'}  OR\par
00050        header->magic[3] != {\cf23 'F'}) \{\par
00051         kprintf({\cf22 "ELF is invalid!\\n"});\par
00052         debug_log({\cf22 "ELF is invalid!"});\par
00053         {\cf19 return} -1;\par
00054     \}\par
00055     \par
00056     elf_get_info(header);\par
00057     \par
00058     kprintf({\cf22 "Type: %s%s%s\\n"},\par
00059         header->arch        == 1 ? {\cf22 "32bit "}:{\cf22 "64 bit"},\par
00060         header->byte_order  == 1 ? {\cf22 "Little Endian "}:{\cf22 "Big endian "},\par
00061         header->elf_version == 1 ? {\cf22 "True ELF"}:{\cf22 "buggy ELF"});\par
00062     {\cf19 if}(header->type != 2) \{\par
00063         kprintf({\cf22 "File is not executable!\\n"});\par
00064         {\cf19 return} 0;\par
00065     \}\par
00066     \par
00067     kprintf({\cf22 "Loading 1/3\\n"});\par
00068 \par
00069     paging_identity_map(0x8048000, info->size);\par
00070     {\cf20 /*  \'CD\'C5 \'D0\'C0\'C1\'CE\'D2\'C0\'C5\'D2!}\par
00071 {\cf20     for(size_t i = 0; i<header->program_header_entries_count; i++) \{}\par
00072 {\cf20         kprintf("Segment [%u/%u]: ", i, header->program_header_entries_count);}\par
00073 {\cf20         elf_program_header_t *prog_header = elf_get_program_header(header, i);}\par
00074 {\cf20 }\par
00075 {\cf20         if (prog_header->type != SEGTYPE_LOAD) \{}\par
00076 {\cf20             kprintf("No load :(?\\n");}\par
00077 {\cf20             continue;}\par
00078 {\cf20         \}}\par
00079 {\cf20 }\par
00080 {\cf20         kprintf("Loading 0x%x bytes to 0x%x\\n", prog_header->mem_size, prog_header->virtual_addr);}\par
00081 {\cf20         memset((void*)prog_header->virtual_addr, 0x90, prog_header->mem_size);}\par
00082 {\cf20         memcpy((void*)prog_header->virtual_addr, }\par
00083 {\cf20                 header + prog_header->offset, }\par
00084 {\cf20                 prog_header->size);}\par
00085 {\cf20     \}}\par
00086 {\cf20     }\par
00087 {\cf20     kprintf("Loading 2/3\\n");}\par
00088 {\cf20 }\par
00089 {\cf20     for(size_t i = 0; i < header->section_header_entries_count; i++) \{}\par
00090 {\cf20         elf_section_t *section_header;}\par
00091 {\cf20         section_header = (elf_section_t*)((uint8_t*)(header) + }\par
00092 {\cf20             header->section_table_position + }\par
00093 {\cf20             header->section_header_entry_size * i);}\par
00094 {\cf20         }\par
00095 {\cf20         kprintf("Section [%u/%u]: ", i, header->section_header_entries_count);}\par
00096 {\cf20 }\par
00097 {\cf20         if(section_header->address) \{}\par
00098 {\cf20             kprintf("name: %u, type: %u, address: 0x%x, offset: %u, size: %u, entry_size: %u!L\\n", }\par
00099 {\cf20                 section_header->name, section_header->type, section_header->address,}\par
00100 {\cf20                 section_header->offset, section_header->size, section_header->entry_size}\par
00101 {\cf20                 );}\par
00102 {\cf20             debug_log("name: %u, type: %u, address: 0x%x, offset: %u, size: %u, entry_size: %u!L", }\par
00103 {\cf20                 section_header->name, section_header->type, section_header->address,}\par
00104 {\cf20                 section_header->offset, section_header->size, section_header->entry_size}\par
00105 {\cf20                 );}\par
00106 {\cf20             data = &header+section_header->offset;}\par
00107 {\cf20             size_t max = section_header->size > 0x1000 ? 0x1000 : section_header->size;}\par
00108 {\cf20 }\par
00109 {\cf20             size_t n = 0;}\par
00110 {\cf20             debug_log("data 0x%x, %u", data, max);}\par
00111 {\cf20             for (size_t i = 0; i < max; i++) \{}\par
00112 {\cf20                 if (data[i] != 0) \{}\par
00113 {\cf20                     debug_log_printf("0x%x ", data[i]);}\par
00114 {\cf20                 \} else \{}\par
00115 {\cf20                     n++;}\par
00116 {\cf20                 \}}\par
00117 {\cf20             \}}\par
00118 {\cf20             debug_log_printf("\\n0x%x, %u, %u, 0x%x\\n", &header+section_header->offset,}\par
00119 {\cf20                 section_header->size > 0x1000 ? 0x1000 : section_header->size,}\par
00120 {\cf20                 n,}\par
00121 {\cf20                 section_header->address}\par
00122 {\cf20             );}\par
00123 {\cf20             memcpy(&section_header->address,}\par
00124 {\cf20                 &header+section_header->offset,}\par
00125 {\cf20                 section_header->size > 0x1000 ? 0x1000 : section_header->size);}\par
00126 {\cf20             memcpy((void*)data, &header+section_header->offset, max);}\par
00127 {\cf20             continue;}\par
00128 {\cf20         \}}\par
00129 {\cf20         kprintf("name: %u, type: %u, address: %u, offset: %u, size: %u, entry_size: %u\\n", }\par
00130 {\cf20                 section_header->name, section_header->type, section_header->address,}\par
00131 {\cf20                 section_header->offset, section_header->size, section_header->entry_size}\par
00132 {\cf20                 );}\par
00133 {\cf20     \}}\par
00134 {\cf20     */}\par
00135     kprintf({\cf22 "[%s] Loading..\\n"}, info->name);\par
00136     debug_log({\cf22 "[%s] Loading.."}, info->name);\par
00137     \par
00138     entry_point = ({\cf18 void}*)(header->entry);\par
00139     \par
00140     kprintf({\cf22 "[%x] entry\\n"}, entry_point);\par
00141     debug_log({\cf22 "[%x] entry"}, entry_point);\par
00142 \par
00143     data = (uint8_t*)({\cf18 void}*)0x8048000;\par
00144     memcpy(data, \par
00145         header, \par
00146         info->size);\par
00147 \par
00148     {\cf19 for} ({\cf18 size_t} i = 0; i < info->size; i++) \{\par
00149         debug_log_printf({\cf22 "0x%x "}, data[i]);\par
00150     \}\par
00151     debug_log_printf({\cf22 "\\n"});\par
00152     {\cf18 int} result = entry_point(syscalls);\par
00153 \par
00154     kprintf({\cf22 "[%s] Return [%u]\\n"}, info->name, result);\par
00155     debug_log({\cf22 "[%s] Return [%u]"}, info->name, result);\par
00156     {\cf19 return} result;\par
00157 \}\par
00158 \par
00159 \par
00160 {\cf18 void} elf_get_info(elf_header_t *header) \{\par
00161     kprintf({\cf22 "ELF file type: %s\\n"},\par
00162               (header->type == ELF_REL) ? \par
00163                 {\cf22 "relocatable"} :\par
00164                 (header->type==ELF_EXEC) ? \par
00165                   {\cf22 "executable"} :\par
00166                   {\cf22 "unknown"}\par
00167     );\par
00168     kprintf({\cf22 "ELF file version: %u\\n"}, header->elf_version);\par
00169     kprintf({\cf22 "Entry point: 0x%x\\n"}, header->entry);\par
00170     kprintf({\cf22 "Program header offset: %u\\n"}, header->header_table_position);\par
00171     kprintf({\cf22 "Section header offset: %u\\n"}, header->section_table_position);\par
00172     kprintf({\cf22 "File flags: %u\\n"}, header->flags);\par
00173     kprintf({\cf22 "File header size: %u\\n"}, header->header_size);\par
00174     kprintf({\cf22 "Program header entry size: %u\\n"}, header->program_header_entries_count);\par
00175     kprintf({\cf22 "Section header entry size: %u\\n"}, header->section_header_entry_size);\par
00176     kprintf({\cf22 "Section header count: %u\\n"}, header->section_header_entries_count);\par
00177     kprintf({\cf22 "Program header count: %u\\n"}, header->program_header_entries_count);\par
00178 \}\par
00179 \par
00180 \par
00181 elf_program_header_t *elf_get_program_header(elf_header_t *header, {\cf18 int} num) \{\par
00182     {\cf19 return} (elf_program_header_t*)(header + header->header_table_position + header->program_header_entry_size * num);\par
00183 \}\par
}
