00060 {\cf18 void} oxygen_merge_blocks(oxygen_mem_entry_t *start) \{\par
00061     {\cf19 if} (!start->free) {\cf19 return};\par
00062     oxygen_mem_entry_t *block = start;\par
00063     {\cf19 while} (block->next && block->next->free) \{\par
00064         block->size += block->next->size + {\cf17 sizeof}(oxygen_mem_entry_t);\par
00065         block->next = block->next->next;\par
00066     \}\par
00067 \}\par
00068 \par
00069 {\cf20 //This function allocates a block of memory.}\par
00070 {\cf18 void} *oxygen_alloc({\cf18 size_t} size) \{\par
00071     {\cf19 return} oxygen_alloc_align(size, 1);\par
00072 \}\par
00073 \par
00074 {\cf18 void} *oxygen_alloc_align({\cf18 size_t} size, {\cf18 size_t} alignment) \{\par
00075     oxygen_mem_entry_t *curr = first_node;\par
00076 \par
00077     {\cf19 while} (curr) \{\par
00078         {\cf19 if} (curr->free) \{\par
00079             {\cf18 void} *addr = curr->data + alignment - 1;\par
00080             addr -= (uintptr_t)addr % alignment + {\cf17 sizeof}(oxygen_mem_entry_t);\par
00081             oxygen_mem_entry_t *second = addr;\par
00082             {\cf19 if} (curr->size >= (second->data - curr->data + size)) \{\par
00083                 oxygen_mem_entry_t *third = (oxygen_mem_entry_t*)(second->data + size);\par
00084 \par
00085                 third->size = curr->size - (third->data - curr->data);\par
00086                 third->next = curr->next;\par
00087                 third->free = 1;\par
00088 \par
00089                 second->size = size;\par
00090                 second->next = third;\par
00091                 second->free = 0;\par
00092 \par
00093                 {\cf19 if} (curr != second) \{\par
00094                     curr->next = second;\par
00095                     curr->size = (uintptr_t)second - (uintptr_t)curr->data;\par
00096                     curr->free = 1;\par
00097                 \}\par
00098 \par
00099                 {\cf19 return} second->data;\par
00100             \}\par
00101         \}\par
00102 \par
00103         curr = curr->next;\par
00104     \}\par
00105 \par
00106     {\cf19 return} NULL;\par
00107 \}\par
00108 \par
00109 {\cf20 //This function releases a block of memory.}\par
00110 {\cf18 void} oxygen_free({\cf18 void} *ptr) \{\par
00111     oxygen_mem_entry_t *curr = first_node, *prev = NULL;\par
00112     {\cf19 while} (curr != NULL) \{\par
00113         {\cf19 if} (curr->data == ptr) \{\par
00114             curr->free = 1;\par
00115             oxygen_merge_blocks(prev ? prev : curr);\par
00116             {\cf19 return};\par
00117         \}\par
00118         prev = curr;\par
00119         curr = curr->next;\par
00120     \}\par
00121 \}\par
