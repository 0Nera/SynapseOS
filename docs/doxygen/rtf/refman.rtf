00156 {\cf20                 : "=a"(tmp->cs)}\par
00157 {\cf20                 );}\par
00158 {\cf20 }\par
00159 {\cf20 }\par
00160 {\cf20     return &tmp;}\par
00161 {\cf20 \}*/}\par
}

